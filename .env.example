# .env.example

# The port your app will run on
PORT=3000

# Application environment (e.g., dev, staging, production)
APP_ENV=dev

# Hostname for the app (e.g., localhost, 0.0.0.0, or a domain)
APP_HOST=localhost

# Your OpenAI API key (get it from https://platform.openai.com/account/api-keys)
OPENAI_API_KEY=

# OpenAI model to use (e.g., gpt-4o, gpt-4o-mini, gpt-3.5-turbo)
OPENAI_MODEL=gpt-4o-mini
